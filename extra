    st.write("Capacity Factor (%)")
with col2: 
    cf_lower_bound_user_input = st.number_input(label="cf_lower_bound_user_input", value=50, label_visibility="collapsed", key = "cf_lower_bound_user_input")
    st.caption("Lower Bound")
