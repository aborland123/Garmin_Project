st.markdown(
    """
    <style>
      /* Only shrink the captions, leaving everything else alone */
      div[data-testid="stCaptionContainer"] > p {
        font-size: 0.68rem !important;
        line-height: 1 !important;
        margin: 0 !important;
        padding: 0 !important;
      }
    </style>
    """,
    unsafe_allow_html=True
)

