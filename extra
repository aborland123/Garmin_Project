        # Calculating NGCC Other Pumps CO2 Removal BEC Breakdown
        pump_water_wp_condensate_fr_lean_solvent_fr = 0.03 
        worl_lean_solv_fr_absor_kg_hr = 10532649.5845321
        pump_lean_solv_dens_lbs_gal = 8.78120532592598
        worl_solv_mkup_flw_pump_gpm = 7
        worl_water_wash_flw_pump_gpm = 1434.08154304461
        worl_solv_mkup_pec_jun2011 = 76648
