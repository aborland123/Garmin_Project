        ngcc_om_R45 = 0
        ngcc_om_R46 = ngcc_om_O46 / 24 * 8760 * ngcc_om_P46 * (doe_ngcc_om_G15 / 100)
        ngcc_om_R47 = ngcc_om_O47 / 24 * 8760 * ngcc_om_P47 * (doe_ngcc_om_G15 / 100)
        ngcc_om_R48 = ngcc_om_O48 / 24 * 8760 * ngcc_om_P48 * (doe_ngcc_om_G15 / 100)
        ngcc_om_R49 = ngcc_om_O49 / 24 * 8760 * ngcc_om_P49 * (doe_ngcc_om_G15 / 100)
        ngcc_om_R50 = ngcc_om_R44 + ngcc_om_R45 + ngcc_om_R46 + ngcc_om_R47 + ngcc_om_R48 + ngcc_om_R49
        ngcc_om_R52 = 0
        ngcc_om_R53 = ngcc_om_R52
        ngcc_om_R54 = ngcc_om_R53 + ngcc_om_R50 + ngcc_om_R42 + ngcc_om_R30
        ngcc_result_summmary_C45 = ngcc_om_R54 / (ngcc_result_summmary_C11 * ngcc_result_summmary_C41 * ngcc_result_summmary_C38)
        ngcc_result_summmary_C60 = (ngcc_result_summmary_E45 - ngcc_result_summmary_C45) / (ngcc_result_summmary_E39 / ngcc_result_summmary_E11 / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_var_op = ngcc_result_summmary_C60

        st.write(f"CO2 Var op value is {ngcc_result_summary_co2_var_op} should be 5.344 = 144.97")


        #CO2 Price - Fuel
        ngcc_om_P56 = ngcc_process_D39
        ngcc_om_O56 = doe_ngcc_om_K60 * Flue_Gas_Flow_Rate_After_DCC_UserInput / NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B
        ngcc_om_R56 = ngcc_om_O56 / 24 * 8760 * ngcc_om_P56 * ngcc_om_S12 / 100
        ngcc_om_R57 = ngcc_om_R56
        ngcc_result_summmary_C46 = ngcc_om_R57 / (ngcc_result_summmary_C11 * ngcc_result_summmary_C41 * ngcc_result_summmary_C38)
        ngcc_result_summmary_C61 = (ngcc_result_summmary_E46 - ngcc_result_summmary_C46) / (ngcc_result_summmary_E39 / ngcc_result_summmary_E11 / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_fuel = ngcc_result_summmary_C61

        st.write(f"CO2 Fuel value is {ngcc_result_summary_co2_fuel} should be 14.26 = 525.28")


        #CO2 Price - T&S
        ngcc_result_summmary_C48 = 0
        ngcc_result_summmary_C62 = (ngcc_result_summmary_E48 - ngcc_result_summmary_C48) / (ngcc_result_summmary_E39 / ngcc_result_summmary_E11 / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_t_s = ngcc_result_summmary_C62

        st.write(f"CO2 T&S value is {ngcc_result_summary_co2_t_s} should be 10 = 10.00")


        if st.session_state["show_calculate_outputs_ngcc"]: 
            try:


                st.subheader(f"CALCULATED RESULT")
                
                # Making the first two boxes with labels and values
                left_labels = ["DCC", "Absorber", "Stripper", "Heavy Haul - Allowance", "Cross HX", "Reboiler", "Condenser", "all other HXs", "Lean Pump"]
                left_values = [NGCC_Results_CO2_Removal_Breakdown_DCC, ngcc_result_summary_absorber, ngcc_result_summary_stripper, ngcc_result_summary_heavy_haul, ngcc_result_summary_cross_hx, ngcc_result_summary_reboiler, ngcc_result_summary_condenser, ngcc_result_summmary_all_other_hxs, ngcc_result_summmary_lean_pump]

                right_labels = ["Rich Pump", "Intercooling Pump, mid", "Other pumps", "Blower", "Reclaimer", "CO2 condenser drum", "Solvent tank", "Miscellaneous", "Allowance for Interconnections"]
                right_values = [ngcc_result_summmary_rich_pump, ngcc_result_summmary_intercooling_pump_mid, ngcc_result_summmary_other_pumps, ngcc_result_summmary_blower, ngcc_result_summmary_reclaimer, ngcc_result_summmary_co2_condenser_drum, ngcc_result_summmary_solvent_tank, ngcc_result_summmary_miscellaneous, ngcc_result_summmary_allowance_for_interconnections]


                # I can use round int on these values if she wants them rounded
                left_col, right_col = st.columns(2)
                with left_col:
                    # Making the box a different background color and had to inject the data through CSS
                    st.markdown(
                        f"""
                        <div style="background-color: #faf0e6; padding: 15px; border-radius: 8px;">
                        {"".join([
                            f"<div style='display: flex; justify-content: space-between; width: 100%;'>"
                            f"<strong>{label}</strong>"
                            f"<span>{value if value is not None else 0:,.2f}</span>"
                            f"</div>"
                            for label, value in zip(left_labels, left_values)
                        ])}
                        </div>
                        """,
                        unsafe_allow_html=True
                    )

                    with right_col:
                        # Making the box a different background color and had to inject the data through CSS
                        st.markdown(
                            f"""
                            <div style="background-color: #faf0e6; padding: 15px; border-radius: 8px;">
                            {"".join([
                                f"<div style='display: flex; justify-content: space-between; width: 100%;'>"
                                f"<strong>{label}</strong>"
                                f"<span>{value if value is not None else 0:,.2f}</span>"
                                f"</div>"
                                for label, value in zip(right_labels, right_values)
                            ])}
                            </div>
                            """,
                            unsafe_allow_html=True
                        )
                    
                #making space between objects
                st.write("")
                st.write("")



                # making the first graph
                all_calculated_result_labels = left_labels + right_labels
                all_calculated_result_values = left_values + right_values
                colors = ["#1dedca", "#e0db32", "#40a816", "#1d2bed", "#ed1d1d", "#bc1ded", 
                        "#1dedca", "#e0db32", "#40a816", "#1d2bed", "#ed1d1d", "#bc1ded", 
                        "#1dedca", "#e0db32", "#40a816", "#1d2bed", "#ed1d1d", "#bc1ded"]

                # using plotly
                fig = go.Figure()
                fig.add_trace(go.Bar(
                    y=all_calculated_result_labels,   
                    x=all_calculated_result_values,  
                    orientation='h',   
                    marker=dict(color=colors)
                ))

                fig.update_layout(
                    title="BEC of CO2 Capture System Component ($*1000)",
                    xaxis_title="",
                    yaxis=dict(autorange="reversed"),
                    template="plotly_white",  # Clean background style
                    autosize=True,
                )
                st.plotly_chart(fig, use_container_width = True)

            


                # Collecting the labels and values for the second box
                left_labels_lcoe = ["Capital", "Fixed op", "Var op", "Fuel", "T&S"]
                left_values_lcoe = [ngcc_result_summary_lcoe_capital, ngcc_result_summary_lcoe_fixed_op, ngcc_result_summary_lcoe_var_op, ngcc_result_summary_lcoe_fuel, ngcc_result_summary_lcoe_t_s]

                right_labels_co2 = ["Capital", "Fixed op", "Var op", "Fuel", "T&S"]
                right_values_co2 = [ngcc_result_summary_co2_capital, ngcc_result_summary_co2_fixed_op, ngcc_result_summary_co2_var_op, ngcc_result_summary_co2_fuel, ngcc_result_summary_co2_t_s]



                left_col, right_col = st.columns(2)
                with left_col:
                    # Making the box a different background color and injecting the data through CSS
                    st.markdown(
                        f"""
                        <div style="background-color: #faf0e6; padding: 15px; border-radius: 8px;">
                        <h6 style="text-align: center; margin-bottom: 1px;">LCOE ($/MWH)</h6>
