        worl_blower_work_hp = worl_dcc_coal_cost_jun117 * worl_blower_gas_flow / 1714
        pump_blower_work_hp = pump_blower_gas_flow_each_blower_gpm * pump_blower_design_dp / 1714 / pump_and_blower_D132
        other_equ_cond_drum_bec_jan_2011 = other_equ_cond_drum_bec_jan_2011_ratio_lean_solv_fr * doe_base_b12b_2015_fuel_feed_rate / doe_base_b12b_2019_fuel_feed_rate
        other_equ_rec_bec_jan_2012 = other_equ_rec_bec_jan_2012_ratio_lean_solv_fr * doe_base_b12b_2015_fuel_feed_rate / doe_base_b12b_2019_fuel_feed_rate
        hx_condenser_total_bec_dec_18 = hx_condenser_total_bec_jun_07 * cost_indx_value_from_UserInput / cepci_2007_jun
        hx_reboiler_total_bec_dec_2018 = hx_reboiler_total_bec_jun_2011 * cost_indx_value_from_UserInput / hx_assump_jun2011
        hx_cross_total_bec_dec_2018 = hx_cross_total_bec_jun_2011 * cost_indx_value_from_UserInput / hx_assump_jun2011
        col_total_bec_dec_2018 = col_heavy_haul_allow_1st_q2018 * cost_indx_value_from_UserInput / cepci_2014 
        column_stripper_total_bec_dec2018 = column_stripper_bec_1q2018 * cost_indx_value_from_UserInput / column_assumptions_1q2018
        column_absorber_total_bec_dec_2018 = column_absorber_bec_1st_q2018 * cost_indx_value_from_UserInput / column_assumptions_1q2018
        col_dcc_bec_cost_dec_2018 = worl_column_dcc_bec_cost_jun11 * cost_indx_value_from_UserInput / cepci_2011_jun                                                 
        aspen_lean_solv_flow_to_absorber = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
        proc_lean_solv_fr_absor = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
        hx_lean_cooler_duty_mmbtu_hr = aspen_lean_cooler_duty_mmbtu_hr * (proc_lean_solv_fr_absor / aspen_lean_solv_flow_to_absorber)
        hx_lean_cooler_area_ft2 = hx_lean_cooler_duty_mmbtu_hr / worl_lean_cooler_lmtd_f / worl_lean_cooler_ohtc_btu_ft2hF * 1000000
        hx_lean_cooler_num = math.ceil(hx_lean_cooler_area_ft2 / 5000)
        hx_lean_cooler_area_per_unit_ft2 = hx_lean_cooler_area_ft2 / hx_lean_cooler_num
        hx_lean_cooler_pec_per_unit_jun_2011 = worl_lean_dcc_ww_cooler_pec_jun2011 * (hx_lean_cooler_area_per_unit_ft2 / worl_lean_dcc_ww_cooler_area_ft2) ** hx_assump_scale_factor
        hx_lean_cooler_bec_per_unit_jun_2011 = hx_lean_cooler_pec_per_unit_jun_2011 * worl_lean_dcc_ww_cooler_bec_jun2011 / worl_lean_dcc_ww_cooler_pec_jun2011
        hx_lean_cooler_total_bec_jun_2011 = hx_lean_cooler_bec_per_unit_jun_2011 * hx_lean_cooler_num
        hx_lean_cooler_total_bec_dec_2018 = hx_lean_cooler_total_bec_jun_2011 * cost_indx_value_from_UserInput / hx_assump_jun2011
        pump_blower_pec_each_jun_2011 = pump_blower_work_hp / worl_blower_work_hp * worley_uky_D192
        other_equ_cond_tot_bec_jun_2011 = other_equ_cond_drum_bec_jan_2011 * cost_indx_value_from_UserInput / cepci_2011_jun
        other_equ_rec_tot_bec_jun_2011 = other_equ_rec_bec_jan_2012 * cost_indx_value_from_UserInput / cepci_D24
        pump_blower_total_pec_jun_2011 = pump_blower_pec_each_jun_2011 * pump_blower_tot_gas_flow
        pump_blower_total_bec_jun_2011 = pump_blower_total_pec_jun_2011 * worley_uky_D193 / worley_uky_D192
        pump_blower_total_bec_dec_2018 = pump_blower_total_bec_jun_2011 * cost_indx_value_from_UserInput / pump_blower_2011_jun
        other_equ_misc_total_bec_dec_2018 = other_equipment_D49 * cost_indx_value_from_UserInput / cepci_2011_jun
        other_equ_bec_solv_makeup_tank = other_equipment_D54 * cost_indx_value_from_UserInput / cepci_D16
        other_equ_bec_solv_rec_tank = other_equipment_D59 * cost_indx_value_from_UserInput / cepci_cost_index_2018_q1_avg
        other_equ_bec_cond_tank = other_equipment_D65 * other_equipment_D66
        capital_bec_misc = other_equ_misc_total_bec_dec_2018 + other_equ_bec_solv_makeup_tank + other_equ_bec_solv_rec_tank + other_equ_bec_cond_tank
        other_equ_sst_total_bec_dec_2018 = other_equ_sst_bec_each_dec_2018 * worl_sst_num_sst
        hx_dcc_cool_tot_bec_dec_2018 = pump_dcc_total_bec_jun_2011 * cost_indx_value_from_UserInput / hx_assump_jun2011
        capital_bec_pumps_tot = NGCC_Pump_And_Blower_Lean_Pumps_Total_BEC_Dec_2018 + NGCC_Pump_And_Blower_Rich_Pumps_Total_BEC_Dec_2018 + pump_intercool_pumps_tot_bec_dec_2018 + capital_bec_solv_makeup_pump + capital_bec_ww_pump + capital_bec_dcc_pump + capital_bec_cond_pump
        capital_bec_hx_tot = hx_cross_total_bec_dec_2018 + hx_reboiler_total_bec_dec_2018 + hx_condenser_total_bec_dec_18 + hx_intercool_tot_bec_dec_2018 + hx_lean_cooler_total_bec_dec_2018 + hx_dcc_cool_tot_bec_dec_2018 + hx_ww_cooler_total_bec_dec_2018
        capital_bec_col_tot = col_dcc_bec_cost_dec_2018 + column_absorber_total_bec_dec_2018 + column_stripper_total_bec_dec2018 + col_total_bec_dec_2018
        capital_bec_remov_sub_total = capital_bec_col_tot + capital_bec_hx_tot + capital_bec_pumps_tot + pump_blower_total_bec_dec_2018 + other_equ_rec_tot_bec_jun_2011 + other_equ_cond_tot_bec_jun_2011 + other_equ_sst_total_bec_dec_2018 + capital_bec_misc
        ngcc_result_summmary_allowance_for_interconnections = (capital_bec_remov_sub_total * 0.15) / (10 ** 3)


