        # Calculating NGCC Solvent tank CO2 Removal BEC Breakdown
        worl_sst_sf = 0.9 # Solvent Surge Tank Scale Factor
        worl_sst_tfr_gpm = 18287.79 # Solvent Surge Tank Total Flow Rate gpm
