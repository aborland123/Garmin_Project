    doe_NGCC_capital_E54 = 316
    doe_NGCC_capital_E55 = 2176
    doe_NGCC_capital_E56 = 84
    doe_NGCC_capital_E57 = 400
    doe_NGCC_capital_E58 = 0
    doe_NGCC_capital_E60 = 1865
    doe_NGCC_capital_F52 = 1454
    doe_NGCC_capital_F53 = 2128
