if "show_calculate_sensitivity_outputs_ngcc" not in st.session_state:
        st.session_state["show_calculate_sensitivity_outputs_ngcc"] = False
if "show_calculate_sensitivity_outputs_sc_pc" not in st.session_state:
        st.session_state["show_calculate_sensitivity_outputs_sc_pc"] = False
left, center, right = st.columns([3,2,3])
with center:
    if st.button("Calculate Sensitivity"):
        if flue_gas == "NGCC":
            st.session_state["show_calculate_sensitivity_outputs_ngcc"] = True
        if flue_gas == "SC PC":
            st.session_state["show_calculate_sensitivity_outputs_sc_pc"] = True
        st.write("Scroll Down")

st.divider()

if flue_gas == 'NGCC':
    try: 
        # Constants Being Read-In From Excel Sheet
        constants = pd.read_excel('Draft1_WebBasedTool.xlsx', sheet_name= 'NGCC_Constants')

        # Calculating NGCC DGG CO2 Removal BEC Breakdown

        # NGCC DGG CO2 Removal BEC Breakdown Constants 
        NGCC_CEPCI_2011Jun = constants.iloc[4, 1] # starts at 0
        NGCC_Column_DCC_Cost_Scale_Factor = constants.iloc[5, 1]
        NGCC_Worley_DCC_Cost_Coal_Jun11 = constants.iloc[6, 1]
        NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B = constants.iloc[7, 1]
        NGCC_Worley_Flue_Gas_Flow_Rate_Before_DCC_ACFM = constants.iloc[8, 1]
        NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM_DOEB31B = constants.iloc[9, 1]

        def BEC_cost(coal_cost, flue_gas_fr_bef, worley_flue_gas_fr_bef, scale_factor):
            return coal_cost * (flue_gas_fr_bef / worley_flue_gas_fr_bef) ** scale_factor


        # Function To calculate NGCC DGG CO2
        def Calculate_NGCC_DCC_CO2_Removal():
            NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM = Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM_DOEB31B / NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B
            
            NGCC_Column_DCC_BEC_Cost_June2011 = NGCC_Worley_DCC_Cost_Coal_Jun11 * (NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM / NGCC_Worley_Flue_Gas_Flow_Rate_Before_DCC_ACFM) ** NGCC_Column_DCC_Cost_Scale_Factor  
            
            NGCC_Column_DCC_BEC_Cost_Dec2018 = NGCC_Column_DCC_BEC_Cost_June2011 * Cost_Index_Value_From_UserInput / NGCC_CEPCI_2011Jun 
            return NGCC_Column_DCC_BEC_Cost_Dec2018 / 1000 

        st.write(f"{Calculate_NGCC_DCC_CO2_Removal()} should be 31,305")




        # Calculating NGCC Absorber CO2 Removal BEC Breakdown

        # Constants
        NGCC_Absorber_Fixed_Design_Point_ColumnG_ibs_ft2_s = constants.iloc[13, 1]
        NGCC_Absorber_WW_Packing_Height_Ft = constants.iloc[14, 1]
        NGCC_Aspen_Packing_Section = constants.iloc[15, 1]
        NGCC_Aspen_Columns_Packing_Height = constants.iloc[16, 1]
        NGCC_Columns_Aspen_Number_Of_Absorber = constants.iloc[17, 1]
        NGCC_Aspen_Columns_Diameter = constants.iloc[18, 1]
        NGCC_Aspen_Columns_Cost_1st_Q2018_PEC_for_2 = constants.iloc[19, 1]
        NGCC_Column_Cost_Scale_Factor = constants.iloc[20, 1]
        NGCC_CEPCI_Cost_Index_2018_1st_Quater_Average = constants.iloc[21, 1]

        # Function To calculate NGCC DGG CO2
        def Calculate_NGCC_Absorber_CO2_Removal():
            Cost_Index_Value_From_UserInput = Cost_Index_Value_From_UserInput 
            NGCC_Absorber_Flue_Gas_Flow_Rate_kg_hr = Flue_Gas_Flow_Rate_After_DCC_UserInput
            NGCC_Absorber_Flue_Gas_Flow_Rate_lbs_s = NGCC_Absorber_Flue_Gas_Flow_Rate_kg_hr * 2.20462 / 3600
            NGCC_Absorber_Cross_Section_Area_Original_ft2 = NGCC_Absorber_Flue_Gas_Flow_Rate_lbs_s / NGCC_Absorber_Fixed_Design_Point_ColumnG_ibs_ft2_s  
            NGCC_Absorber_Diameter_original_ft = math.sqrt(NGCC_Absorber_Cross_Section_Area_Original_ft2 / math.pi) * 2 
            L_G_Process_Parameters_UserInput = L_G_Process_Parameters_UserInput
            NGCC_Absorber_Diameter_m = NGCC_Absorber_Diameter_original_ft * 0.3048
            NGCC_Absorber_Fixed_Design_Packing_Height_ft = 130.95 * L_G_Process_Parameters_UserInput - 82.133
            NGCC_Absorber_Number_Of_Column = 1 if NGCC_Absorber_Diameter_m < 30.48 else 2
            NGCC_Absorber_Number_Packing_Section = round(NGCC_Absorber_Fixed_Design_Packing_Height_ft / 12, 0)
            NGCC_Absorber_Cross_Section_Area_ft2 = NGCC_Absorber_Flue_Gas_Flow_Rate_lbs_s / NGCC_Absorber_Fixed_Design_Point_ColumnG_ibs_ft2_s / NGCC_Absorber_Number_Of_Column
            NGCC_Absorber_Total_Height = NGCC_Absorber_Fixed_Design_Packing_Height_ft + NGCC_Absorber_WW_Packing_Height_Ft + NGCC_Absorber_Number_Packing_Section * 10 + 25
            NGCC_Absorber_Diameter_ft = math.sqrt(NGCC_Absorber_Cross_Section_Area_ft2 / math.pi) * 2
            NGCC_Absorber_Height_Packing_Height_ft = NGCC_Absorber_Total_Height
            NGCC_Absorber_Diameter_ft = NGCC_Absorber_Diameter_ft
            NGCC_Absorber_Number_Of_Absorber_ft = NGCC_Absorber_Number_Of_Column
            NGCC_Aspen_Columns_Total_Height_ft = NGCC_Aspen_Columns_Packing_Height + NGCC_Aspen_Packing_Section * 10 + 10 + 25
            NGCC_Column_Absorber_Packing_Height_ft = NGCC_Absorber_Height_Packing_Height_ft
            NGCC_Column_Absorber_Diameter_ft = NGCC_Absorber_Diameter_ft
            NGCC_Column_Absorber_Number_Of_Absorber = NGCC_Absorber_Number_Of_Absorber_ft
            NGCC_Aspen_Columns_Volume_ft3 = NGCC_Aspen_Columns_Total_Height_ft * (NGCC_Aspen_Columns_Diameter/ 2) ** 2 * math.pi * NGCC_Columns_Aspen_Number_Of_Absorber
            NGCC_Column_Absorber_Volume_Of_Absorber_ft3 = NGCC_Column_Absorber_Number_Of_Absorber * (NGCC_Column_Absorber_Diameter_ft / 2) ** 2 * math.pi * NGCC_Column_Absorber_Packing_Height_ft
            NGCC_Column_Assumptions_1st_Q2018 = NGCC_CEPCI_Cost_Index_2018_1st_Quater_Average
            NGCC_Column_Absorber_Cost_By_CostModel_1st_Q2018 = (NGCC_Column_Absorber_Volume_Of_Absorber_ft3 / NGCC_Aspen_Columns_Volume_ft3) ** NGCC_Column_Cost_Scale_Factor * NGCC_Aspen_Columns_Cost_1st_Q2018_PEC_for_2
            NGCC_Column_Absorber_BEC_1st_Q2018 = NGCC_Column_Absorber_Cost_By_CostModel_1st_Q2018 * 1.38
            NGCC_Column_Absorber_Total_BEC_Dec_2018 = NGCC_Column_Absorber_BEC_1st_Q2018 * Cost_Index_Value_From_UserInput / NGCC_Column_Assumptions_1st_Q2018  
            return NGCC_Column_Absorber_Total_BEC_Dec_2018 / 1000 

        st.write(f"{Calculate_NGCC_Absorber_CO2_Removal()} should be 74,923")



        # Calculating NGCC Stripper CO2 Removal BEC Breakdown                                                                              
        NGCC_Process_CO2_Mol_Frac_After_DCC = constants.iloc[25, 1]
        NGCC_Stripper_Liguid_Flow_Rate_Density_kg_m3 = constants.iloc[26, 1]
        NGCC_Stripper_Operating_Conditions_Residence_Time_min = constants.iloc[27, 1]
        NGCC_Stripper_Volume_Of_Vessel_G_ft3 = constants.iloc[28, 1]
        NGCC_Aspen_Columns_Packing_Section = constants.iloc[29, 1]
        NGCC_Aspen_Columns_Packing_Height_ft = constants.iloc[30, 1]
        NGCC_Column_Stripper_Number_Of_Stripper = constants.iloc[31, 1]
        NGCC_Aspen_Columns_Cost_By_Aspen_1st_Q2018_PEC_2 = constants.iloc[32, 1]
        NGCC_Aspen_Columns_Diameter_ft = constants.iloc[33, 1]
        NGCC_Column_Cost_Scale_Factor = constants.iloc[34, 1]
        NGCC_CEPCI_Cost_Index_2018_1st_Quater_Average = constants.iloc[35, 1]


        def Calculate_NGCC_Stripper_CO2_Removal():
            co2_capture_rate_user_input = co2_capture_rate_user_input
            NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber = Flue_Gas_Flow_Rate_After_DCC_UserInput * L_G_Process_Parameters_UserInput
            NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_hr = NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber + Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_CO2_Mol_Frac_After_DCC * co2_capture_rate_user_input
            NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_s = NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_hr / 3600
            NGCC_Stripper_Liquid_Flow_Rate_Liquid_Flow_Rate_l_min = (NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_s * 1000 * 60) / (NGCC_Stripper_Liguid_Flow_Rate_Density_kg_m3)
            NGCC_Stripper_Volume_Of_Vessel_L = 2 * NGCC_Stripper_Operating_Conditions_Residence_Time_min * NGCC_Stripper_Liquid_Flow_Rate_Liquid_Flow_Rate_l_min 
            NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_lbs_s = NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_hr * 2.20462 / 3600
            NGCC_Stripper_Volume_Of_Vessel_ft3 = NGCC_Stripper_Volume_Of_Vessel_L * 0.001 * 35.3147
            NGCC_Stripper_Dimension_Cross_Section_ft2 = NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_lbs_s / NGCC_Stripper_Volume_Of_Vessel_G_ft3
            NGCC_Stripper_Dimension_Length_ft = NGCC_Stripper_Volume_Of_Vessel_ft3 / NGCC_Stripper_Dimension_Cross_Section_ft2
            NGCC_Stripper_Dimension_Diameter_ft = math.sqrt(NGCC_Stripper_Dimension_Cross_Section_ft2 / math.pi) * 2
            NGCC_Aspen_Columns_Total_Height_ft = NGCC_Aspen_Columns_Packing_Height_ft + NGCC_Aspen_Columns_Packing_Section * 8 + 15
            NGCC_Column_Stripper_Total_Height_ft = NGCC_Stripper_Dimension_Length_ft
            NGCC_Column_Stripper_Diameter_ft = NGCC_Stripper_Dimension_Diameter_ft
            NGCC_Aspen_Columns_Total_Volume_ft3 = NGCC_Aspen_Columns_Total_Height_ft * (NGCC_Aspen_Columns_Diameter_ft / 2) ** 2 * math.pi
            NGCC_Column_Stripper_Volume_Of_Stripper_ft3 = NGCC_Column_Stripper_Number_Of_Stripper * (NGCC_Column_Stripper_Diameter_ft / 2) ** 2 * math.pi * NGCC_Column_Stripper_Total_Height_ft
            Cost_Index_Value_From_UserInput = Cost_Index_Value_From_UserInput
            NGCC_Column_Assumptions_1st_Q2018 = NGCC_CEPCI_Cost_Index_2018_1st_Quater_Average
            NGCC_Column_Stripper_Cost_By_Cost_Model_1st_Q2018 = (NGCC_Column_Stripper_Volume_Of_Stripper_ft3 / NGCC_Aspen_Columns_Total_Volume_ft3) ** NGCC_Column_Cost_Scale_Factor * NGCC_Aspen_Columns_Cost_By_Aspen_1st_Q2018_PEC_2
            NGCC_Column_Stripper_BEC_1st_Q2018 = NGCC_Column_Stripper_Cost_By_Cost_Model_1st_Q2018 * 1.45
            NGCC_Column_Stripper_Total_BEC_Dec2018 = NGCC_Column_Stripper_BEC_1st_Q2018 * Cost_Index_Value_From_UserInput / NGCC_Column_Assumptions_1st_Q2018
            return NGCC_Column_Stripper_Total_BEC_Dec2018 / 1000    

        # Display output in Streamlit
        st.write(int(round(Calculate_NGCC_Stripper_CO2_Removal(), 0)))
        st.write("Stripper Result: 11,542 for placement 4.14%. 14,378 for 40%")




