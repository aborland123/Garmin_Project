            NGCC_Pump_And_Blower_Intercooling_Pumps_PEC_For_Each_June_2011 = (NGCC_Pump_And_Blower_Intercooling_Pumps_Work_For_Each_hp / NGCC_Worley_Intercooling_Pumps_Work_hp) ** NGCC_Pump_And_Blower_Pump_Scale_Factor * NGCC_Worley_Intercooler_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Intercooling_Pumps_BEC_For_Each_Op_June_2011 = NGCC_Pump_And_Blower_Intercooling_Pumps_PEC_For_Each_June_2011 * NGCC_Worley_Intercooler_Pumps_BEC_Jun_2011 / NGCC_Worley_Intercooler_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Intercooling_Pumps_BEC_For_Each_Spare_June_2011 = NGCC_Pump_And_Blower_Intercooling_Pumps_PEC_For_Each_June_2011 * NGCC_Worley_Intercooler_Pumps_BEC_For_Spare_Jun_2011 / NGCC_Worley_Intercooler_Pumps_PEC_June_2011
