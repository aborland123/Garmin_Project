        sc_pc_stripper_sizing_D34 = sc_pc_stripper_sizing_D30 / sc_pc_stripper_sizing_D35
        sc_pc_worley_uky_D139 = 1095 * sc_pc_worley_uky_D247 * (sc_pc_worley_uky_D138) * sc_pc_worley_uky_D248 * sc_pc_worley_uky_D249
        sc_pc_pump_and_blower_D53 = sc_pc_worley_uky_D140 - sc_pc_worley_uky_D139
        sc_pc_stripper_sizing_D9 = sc_pc_stripper_sizing_D34
        sc_pc_pump_and_blower_D57 = sc_pc_pump_and_blower_D56 / sc_pc_pump_and_blower_D51
        sc_pc_pump_and_blower_D54 = 1095 * 9.8 * (sc_pc_stripper_sizing_D9) * 0.3048 * 0.000145038 + sc_pc_pump_and_blower_D53
        sc_pc_pump_and_blower_D59 = sc_pc_pump_and_blower_D57 * sc_pc_pump_and_blower_D54 / 1714 / sc_pc_pump_and_blower_D58
        sc_pc_worley_uky_D133 = sc_pc_worley_uky_D131 * sc_pc_worley_uky_D132 / 1714
        sc_pc_pump_and_blower_D60 = (sc_pc_pump_and_blower_D59 / sc_pc_worley_uky_D133) ** sc_pc_pump_and_blower_C11 * sc_pc_worley_uky_D134
        sc_pc_pump_and_blower_D62 = sc_pc_pump_and_blower_D60 * sc_pc_worley_uky_D136 / sc_pc_worley_uky_D134 
        sc_pc_pump_and_blower_D61 = sc_pc_pump_and_blower_D60 * sc_pc_worley_uky_D135 / sc_pc_worley_uky_D134
        sc_pc_pump_and_blower_C10 = cost_indx_value_from_UserInput
        sc_pc_pump_and_blower_D63 = sc_pc_pump_and_blower_D61 * sc_pc_pump_and_blower_D51 + sc_pc_pump_and_blower_D62
        sc_pc_result_summmary_rich_pump = (sc_pc_pump_and_blower_D63 * sc_pc_pump_and_blower_C10 / sc_pc_pump_and_blower_C12) / 1000

