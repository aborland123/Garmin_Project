        ngcc_result_summary_lcoe_var_op = ngcc_result_summmary_E45

        st.write(f"LCOE Var op value is {ngcc_result_summary_lcoe_var_op} should be 4.2497 -3.0")


        #LCOE - Fuel
        ngcc_result_summmary_E46 = ngcc_om_AD57 / (ngcc_result_summmary_E11 * ngcc_result_summmary_E41 * ngcc_result_summmary_E38)
        ngcc_result_summary_lcoe_fuel = ngcc_result_summmary_E46

        st.write(f"LCOE Fuel value is {ngcc_result_summary_lcoe_fuel} should be 33.3 -5.9")



