        cap_ref_tpc_new_build_buildings_struct_1000 = cap_ref_tpc_b31a_buildings_struct_1000 * (flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr) ** 0.8
        cap_ref_tpc_2018 = (0 + 0 + cap_ref_tpc_new_build_feed_wat_misc_bop_sys_1000 + 0 + 0 
                            + cap_ref_tpc_new_build_comb_turb_acc_1000 + cap_ref_tpc_new_build_hrsg_ductwork_stack_1000 + cap_ref_tpc_new_build_stm_turb_acc_1000 + cap_ref_tpc_new_build_cooling_wat_sys_1000 + 0 
                            + cap_ref_tpc_new_build_acc_electr_plant_1000 + cap_ref_tpc_new_build_instrumentation_control_1000 + cap_ref_tpc_new_build_improv_site_1000 + cap_ref_tpc_new_build_buildings_struct_1000) * 1000
        cap_ref_combst_turb_power_mwe_w_o_co2_cap = ngcc_inputs_D96
        cap_ref_stm_turb_power_mwe_w_o_co2_cap = cap_ref_stm_turb_power_mwe_b31a * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        cap_ref_total_aux_mwe_w_o_co2_cap = cap_ref_total_aux_mwe_b31a * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        cap_ref_pow_out_w_o_ccs_net_pow_w_o_carb_capt = cap_ref_combst_turb_power_mwe_w_o_co2_cap + cap_ref_stm_turb_power_mwe_w_o_co2_cap - cap_ref_total_aux_mwe_w_o_co2_cap
        cap_ref_toc_other_financ_cost_kw = ngcc_capital_C64 / cap_ref_pow_out_w_o_ccs_net_pow_w_o_carb_capt
        cap_ref_toc_inv_cost_60_day_fuel_full_capacity = (ngcc_om_G42 - ngcc_om_G33) / 12 / 1000 * 2 / (ngcc_om_H12 / 100)
        cap_ref_toc_inv_cost_total_1000 = cap_ref_toc_inv_cost_60_day_fuel_full_capacity + cost_indx_value_from_UserInput8
        cap_ref_toc_pre_prod_cost_total_1000 = cost_indx_value_from_UserInput9 + cost_indx_value_from_UserInput0 + cost_indx_value_from_UserInput1 + cost_indx_value_from_UserInput2 + cost_indx_value_from_UserInput3 + cost_indx_value_from_UserInput4
        cap_ref_tot_proj_cost_dec_2018 = cap_ref_tpc_2018 *cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        cap_ref_pow_out_w_o_ccs_net_pow_w_o_carb_capt = cap_ref_combst_turb_power_mwe_w_o_co2_cap + cap_ref_stm_turb_power_mwe_w_o_co2_cap - cap_ref_total_aux_mwe_w_o_co2_cap
        doe_base_est_aux_load_tot_aux_mwe = (doe_baseline_inputs_D171 + doe_baseline_inputs_D172 + doe_baseline_inputs_D173 + doe_baseline_inputs_D174 + doe_baseline_inputs_D175 
                                    + doe_baseline_inputs_D176 + doe_baseline_inputs_D177 + doe_baseline_inputs_D178 + doe_baseline_inputs_D179 + doe_baseline_inputs_D180 
                                    + doe_baseline_inputs_D181 + doe_baseline_inputs_D182) / 1000
        doe_base_est_aux_load_net_pow_mwe = 263
        cap_ref_total_aux_mwe_b31a
        cap_ref_stm_turb_power_mwe_b31a
        cap_ref_total_aux_mwe_w_o_co2_cap
        cap_ref_stm_turb_power_mwe_w_o_co2_cap
        cap_ref_combst_turb_power_mwe_w_o_co2_cap
        cap_ref_total_aux_mwe_b31a = doe_base_est_aux_load_tot_aux_mwe
        cap_ref_stm_turb_power_mwe_b31a = doe_base_est_aux_load_net_pow_mwe
        cap_ref_total_aux_mwe_w_o_co2_cap = cap_ref_total_aux_mwe_b31a * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        cap_ref_stm_turb_power_mwe_w_o_co2_cap = cap_ref_stm_turb_power_mwe_b31a * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        cap_ref_combst_turb_power_mwe_w_o_co2_cap = ngcc_inputs_D96
        cap_ref_tasc_multiplier = 1.093
        cap_ref_toc_other_cost_total_1000 = ngcc_capital_C61 + ngcc_capital_C62 + ngcc_capital_C63 + ngcc_capital_C64
        cap_ref_toc_1000 = cap_ref_tot_proj_cost_dec_2018 / 1000 + cap_ref_toc_pre_prod_cost_total_1000 + cap_ref_toc_inv_cost_total_1000 + cap_ref_toc_other_cost_total_1000
        cap_ref_pow_out_w_o_ccs_net_pow_w_o_carb_capt = cap_ref_combst_turb_power_mwe_w_o_co2_cap + cap_ref_stm_turb_power_mwe_w_o_co2_cap - cap_ref_total_aux_mwe_w_o_co2_cap
        cap_ref_tasc = cap_ref_toc_1000 * cap_ref_tasc_multiplier
        res_annual_hour = 8760
        res_net_power_out_w_o_ccs = cap_ref_pow_out_w_o_ccs_net_pow_w_o_carb_capt
        inp_est_aux_load_tot_aux_mwe = (ngcc_inputs_D101 + ngcc_inputs_D102 + ngcc_inputs_D103 + ngcc_inputs_D104 + ngcc_inputs_D105 + ngcc_inputs_D106 
                            + ngcc_inputs_D107 + ngcc_inputs_D108 +ngcc_inputs_D109 + ngcc_inputs_D110 + ngcc_inputs_D111 + ngcc_inputs_D112) / 1000
        inp_est_aux_load_tot_gross_pow_mwe = ngcc_inputs_D98
        inp_est_aux_load_net_power_mwe = inp_est_aux_load_tot_gross_pow_mwe - inp_est_aux_load_tot_aux_mwe
        res_net_power_out_mw = inp_est_aux_load_net_power_mwe
        res_tasc_cost = cap_ref_tasc
        res_lcoe_capital = res_tasc_cost * 1000 * fcr_baseline_user_input / (res_net_power_out_w_o_ccs * res_annual_hour * cf_baseline_user_input)
        res_co2_capital = (ngcc_result_summmary_E43 - res_lcoe_capital) / (ngcc_result_summmary_E39 / res_net_power_out_mw / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_capital = res_co2_capital

        st.write(f"CO2 Capital value is {ngcc_result_summary_co2_capital} should be 42.2208 = 7558.11") 





        #CO2 Price - Fixed op
        ngcc_result_summmary_C44 = ngcc_om_G27 / (res_net_power_out_w_o_ccs * res_annual_hour * cf_baseline_user_input)
        ngcc_result_summmary_C59 = (ngcc_result_summmary_E44 - ngcc_result_summmary_C44) / (ngcc_result_summmary_E39 / res_net_power_out_mw / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_fixed_op = ngcc_result_summmary_C59

        st.write(f"CO2 Fixed op value is {ngcc_result_summary_co2_fixed_op} should be 13.955 = 96627.48")



        #CO2 Price - Var op
        ngcc_om_N40 = lean_pump_tot_flow_gpm * 522 / 2 / 2000 * 0.30 * 1.5
        ngcc_om_O33 = doe_ngcc_om_K36 * ngcc_inputs_D121 / doe_baseline_inputs_E190
        ngcc_om_O34 = doe_ngcc_om_K37 * ngcc_inputs_D121 / doe_baseline_inputs_E190
        ngcc_om_O38 = doe_ngcc_om_K41 * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        ngcc_om_O39 = doe_ngcc_om_K42 * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        ngcc_om_O40 = ngcc_om_N40 * 0.0014
        ngcc_om_O41 = doe_ngcc_om_K44 * ngcc_process_D53 / ngcc_process_E53
        ngcc_om_O46 = doe_ngcc_om_K49 * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        ngcc_om_O47 = ngcc_om_O41
        ngcc_om_O48 = doe_ngcc_om_K51 * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        ngcc_om_P33 = 1.9 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P34 = 550 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P38 = 298.21 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P39 = 150 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P40 = ngcc_process_D40
        ngcc_om_P41 = 6.8 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P46 = 2.5 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P47 = 0.35 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_P48 = 38 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        ngcc_om_R33 = ngcc_om_O33 / 24 * 8760 * ngcc_om_P33 * (ngcc_om_S12 / 100)
        ngcc_om_R34 = ngcc_om_O34 / 24 * 8760 * ngcc_om_P34 * (ngcc_om_S12 / 100)
        oom_co2_rem_voc_ammonia_year_cost = ngcc_om_O38 / 24 * 8760 * ngcc_om_P38 * (ngcc_om_S12 / 100)
        oom_co2_rem_voc_scr_catalyst_year_cost = ngcc_om_O39 / 24 * 8760 * ngcc_om_P39 * (ngcc_om_S12 / 100)
        oom_co2_rem_voc_co2_cap_sys_chemicals_year_cost = ngcc_om_O40 / 24 * 8760 * ngcc_om_P40 * (ngcc_om_S12 / 100)
        oom_co2_rem_voc_teg_year_cost = ngcc_om_O41 / 24 * 8760 * ngcc_om_P41 * (ngcc_om_S12 / 100)
        oom_co2_rem_voc_thermal_rec_waste_day_cost = ngcc_om_O40 * 0.50
        oom_co2_rem_voc_thermal_rec_waste_unit_cost = 38 * cost_indx_value_from_UserInput / cost_indx_value_from_UserInput
        oom_co2_rem_voc_mainten_materials_year_cost = ngcc_capital_N93 * 0.0114
        oom_co2_rem_voc_subtotal_year_cost = ngcc_om_R33 + ngcc_om_R34 + 0 + 0 + 0 + oom_co2_rem_voc_ammonia_year_cost + oom_co2_rem_voc_scr_catalyst_year_cost + oom_co2_rem_voc_co2_cap_sys_chemicals_year_cost + oom_co2_rem_voc_teg_year_cost
        oom_co2_rem_voc_scr_catalyst_waste_year_cost = ngcc_om_O46 / 24 * 8760 * ngcc_om_P46 * (doe_om_cf_percent / 100)
        oom_co2_rem_voc_teg_waste_year_cost = ngcc_om_O47 / 24 * 8760 * ngcc_om_P47 * (doe_om_cf_percent / 100)
        oom_co2_rem_voc_amine_purif_unit_waste_year_cost = ngcc_om_O48 / 24 * 8760 * ngcc_om_P48 * (doe_om_cf_percent / 100)
        oom_co2_rem_voc_thermal_rec_waste_year_cost = oom_co2_rem_voc_thermal_rec_waste_day_cost / 24 * 8760 * oom_co2_rem_voc_thermal_rec_waste_unit_cost * (doe_om_cf_percent / 100)
        oom_co2_rem_voc_subtotal_waste_year_cost = 0 + 0 + oom_co2_rem_voc_scr_catalyst_waste_year_cost + oom_co2_rem_voc_teg_waste_year_cost + oom_co2_rem_voc_amine_purif_unit_waste_year_cost + oom_co2_rem_voc_thermal_rec_waste_year_cost
        oom_co2_rem_voc_subtotal_by_product_year_cost = 0
        oom_co2_rem_voc_var_operat_total_by_product_year_cost = oom_co2_rem_voc_subtotal_by_product_year_cost + oom_co2_rem_voc_subtotal_waste_year_cost + oom_co2_rem_voc_subtotal_year_cost + oom_co2_rem_voc_mainten_materials_year_cost
        ngcc_result_summmary_C45 = oom_co2_rem_voc_var_operat_total_by_product_year_cost / (res_net_power_out_w_o_ccs * res_annual_hour * cf_baseline_user_input)
        ngcc_result_summmary_C60 = (ngcc_result_summmary_E45 - ngcc_result_summmary_C45) / (ngcc_result_summmary_E39 / res_net_power_out_mw / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_var_op = ngcc_result_summmary_C60

        st.write(f"CO2 Var op value is {ngcc_result_summary_co2_var_op} should be 5.344 = 144.97")


        #CO2 Price - Fuel
        om_co2_rem_fuel_cost_ng_unit = ngcc_process_D39
        om_co2_rem_fuel_cost_ng_day = doe_ngcc_om_K60 * flue_gas_fr_aft_dcc_UserInput / proc_ref_flue_gas_aftr_dcc_kg_hr
        om_co2_rem_fuel_cost_ng_year = om_co2_rem_fuel_cost_ng_day / 24 * 8760 * om_co2_rem_fuel_cost_ng_unit * ngcc_om_S12 / 100
        om_co2_rem_fuel_cost_ng_year_tot = om_co2_rem_fuel_cost_ng_year
        res_lcoe_fuel = om_co2_rem_fuel_cost_ng_year_tot / (res_net_power_out_w_o_ccs * res_annual_hour * cf_baseline_user_input)
        res_co2_price_w_o_ts_fuel = (ngcc_result_summmary_E46 - res_lcoe_fuel) / (ngcc_result_summmary_E39 / res_net_power_out_mw / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_fuel = res_co2_price_w_o_ts_fuel

        st.write(f"CO2 Fuel value is {ngcc_result_summary_co2_fuel} should be 14.26 = 525.28")


        #CO2 Price - T&S
        res_lcoe_ts = 0
        res_co2_price_w_o_ts_ts = (ngcc_result_summmary_E48 - res_lcoe_ts) / (ngcc_result_summmary_E39 / res_net_power_out_mw / ngcc_result_summmary_E41)
        ngcc_result_summary_co2_t_s = res_co2_price_w_o_ts_ts

        st.write(f"CO2 T&S value is {ngcc_result_summary_co2_t_s} should be 10 = 10.00")









        # Calculate Sensativity Analysis

        #CO2 Captured Sensitivity Analysis Baseline #
        def co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ngcc_process_D39, ngcc_process_D40, 
                                        ngcc_process_D41, ngcc_process_D42, ngcc_process_D43, ngcc_process_D44):
            cap_whole_co2_tasc_multip = 1.093
            proc_co2_mass_frac_aftr_dcc = 0.0639
            ngcc_process_D53 = flue_gas_fr_aft_dcc_UserInput * proc_co2_mass_frac_aftr_dcc * co2_capture_rate_user_input
            proc_ref_flue_gas_aftr_dcc_kg_hr = 3889650
            proc_ref_co2_mass_frac_aft_dcc_b31b = 0.0639
            co2_capture_rate_user_input = 0.90
            ngcc_process_E53 = proc_ref_flue_gas_aftr_dcc_kg_hr * proc_ref_co2_mass_frac_aft_dcc_b31b * co2_capture_rate_user_input
            proc_lean_solv_fr_absor = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
            proc_ref_flue_gas_before_dcc_doeb31b = 3927398
            proc_flue_gas_b4_dcc_acfm_DOEB31B = 2531705
            proc_flue_gas_b4_dcc_acfm = flue_gas_fr_aft_dcc_UserInput * proc_flue_gas_b4_dcc_acfm_DOEB31B / proc_ref_flue_gas_aftr_dcc_kg_hr
            proc_co2_mol_frac_aftr_dcc = 0.0414
            cepci_D16 = 615.9
            pump_lean_solv_dens_lbs_gal = 8.78120532592598
            lean_pump_tot_flow_gpm = proc_lean_solv_fr_absor / 60 * 2.2 / pump_lean_solv_dens_lbs_gal
            ngcc_om_N40 = lean_pump_tot_flow_gpm * 522 / 2 / 2000 * 0.30 * 1.5
            doe_NGCC_capital_I52 = 1212
            doe_NGCC_capital_I53 = 1252
            doe_NGCC_capital_I54 = 316
            doe_NGCC_capital_I55 = 2072
            doe_NGCC_capital_I56 = 105
