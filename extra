# Calculating NGCC Lean Pump CO2 Removal BEC Breakdown
pump_and_blower_D41 = 0.83
pump_and_blower_D34 = 2
pump_and_blower_D38 = 8.78120532592598
worley_uky_D127 = 68.5
worley_uky_D241 = 9.8
worley_uky_D125 = 88
worley_uky_D242 = 0.3048
worley_uky_D243 = 0.000145038
worley_uky_D126 = 1095 * worley_uky_D241 * (worley_uky_D125) * worley_uky_D242 * worley_uky_D243
ngcc_process_D31 = ## flue gas user input
ngcc_process_D32 = l_g_user_input ## user input
