absorber_sizing_D31 = 10
absorber_sizing_D32 = round(absorber_sizing_D20 / 12, 0)
absorber_sizing_D33 = absorber_sizing_D20 + absorber_sizing_D31 + absorber_sizing_D32 * 10 + 25
absorber_sizing_D9 = absorber_sizing_D33
worley_uky_D128 = worley_uky_D127 - worley_uky_D126
