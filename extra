            except Exception as e:
                st.error(f"{e}")

    except Exception as e:
        st.error(f"{e}")


if flue_gas == 'SC PC':
    try: 

        # Calculating SC PC DGG CO2 Removal BEC Breakdown - CORRECT!!

        # User inputs
        cost_indx_value_from_UserInput = cost_indx_value_from_UserInput

        # Constants
        sc_pc_cepci_D26 = 588.9
        sc_pc_column_C8 = 0.7
        sc_pc_worley_uky_D18 = 39411710.08
        sc_pc_process_E31 = 3385665 
        sc_pc_worley_uky_D9 = 1393316.12138698
        sc_pc_process_E33 = 3213480
        sc_pc_process_E32 = 1899852.25307286
        flue_gas_fr_aft_dcc_UserInput = flue_gas_fr_aft_dcc_UserInput
        l_g_proc_param_UserInput = flue_gas_fr_aft_dcc_UserInput * sc_pc_process_E32 / sc_pc_process_E33

        # Formulas
        sc_pc_column_D25 = sc_pc_worley_uky_D18 * (l_g_proc_param_UserInput / sc_pc_worley_uky_D9) ** sc_pc_column_C8  
        sc_pc_result_summary_dcc = (sc_pc_column_D25 * cost_indx_value_from_UserInput / sc_pc_cepci_D26) / 1000  

        # Display output in Streamlit
        st.write(int(round(sc_pc_result_summary_dcc, 0)))
        st.write(f"SC PC DGG Result: 58,535 for placement. 95 for 400.")




        # Constants
 
        # sc_pc_result_summary_dcc2 = ((39411710 * ((flue_gas_fr_aft_dcc_UserInput * 25307286 / 3213480) ** 0.7) / 12138698) * cost_indx_value_from_UserInput / 588.9) / 1000  

        # # Display output in Streamlit
        # st.write(int(round(sc_pc_result_summary_dcc2, 0)))



        # Calculating NGCC Absorber CO2 Removal BEC Breakdown
        sc_pc_absorber_sizing_D19 = 0.23
        sc_pc_absorber_sizing_D31 = 10
        sc_pc_aspen_model_results_E50 = 4
        sc_pc_aspen_model_results_E48 = 48
        sc_pc_aspen_model_results_E52 = 2
        sc_pc_aspen_model_results_E49 = 73
        sc_pc_aspen_model_results_E54 = 42012667
        sc_pc_column_C9 = 0.9
        sc_pc_cepci_D17 = 581.8

        sc_pc_absorber_sizing_D15 = flue_gas_fr_aft_dcc_UserInput
        sc_pc_absorber_sizing_D16 = sc_pc_absorber_sizing_D15 * 2.20462 / 3600
        sc_pc_absorber_sizing_D22 = sc_pc_absorber_sizing_D16 / sc_pc_absorber_sizing_D19  
        sc_pc_absorber_sizing_D23 = math.sqrt(sc_pc_absorber_sizing_D22 / math.pi) * 2 
        l_g_proc_param_UserInput = l_g_proc_param_UserInput
        sc_pc_absorber_sizing_D24 = sc_pc_absorber_sizing_D23 * 0.3048

        sc_pc_absorber_sizing_D20 = 33.939 * l_g_proc_param_UserInput ** 2 - 173.98 * l_g_proc_param_UserInput + 252.88

        sc_pc_absorber_sizing_D25 = 1 if sc_pc_absorber_sizing_D24 < 30.48 else 2
        sc_pc_absorber_sizing_D32 = round(sc_pc_absorber_sizing_D20 / 12, 0)
        sc_pc_absorber_sizing_D26 = sc_pc_absorber_sizing_D16 / sc_pc_absorber_sizing_D19 / sc_pc_absorber_sizing_D25
        sc_pc_absorber_sizing_D33 = sc_pc_absorber_sizing_D20 + sc_pc_absorber_sizing_D31 + sc_pc_absorber_sizing_D32 * 10 + 25
        sc_pc_absorber_sizing_D27 = math.sqrt(sc_pc_absorber_sizing_D26 / math.pi) * 2
        sc_pc_absorber_sizing_D9 = sc_pc_absorber_sizing_D33
        sc_pc_absorber_sizing_D8 = sc_pc_absorber_sizing_D27
        sc_pc_absorber_sizing_D10 = sc_pc_absorber_sizing_D25
        sc_pc_aspen_model_results_E51 = sc_pc_aspen_model_results_E48 + sc_pc_aspen_model_results_E50 * 10 + 10 + 25
        sc_pc_column_D29 = sc_pc_absorber_sizing_D9
        sc_pc_column_D30 = sc_pc_absorber_sizing_D8
        sc_pc_column_D31 = sc_pc_absorber_sizing_D10
        sc_pc_aspen_model_results_E53 = sc_pc_aspen_model_results_E51 * (sc_pc_aspen_model_results_E49 / 2) ** 2 * math.pi * sc_pc_aspen_model_results_E52
        sc_pc_column_D32 = sc_pc_column_D31 * (sc_pc_column_D30 / 2) ** 2 * math.pi * sc_pc_column_D29
        sc_pc_cepci_D17 = sc_pc_cepci_D17
        sc_pc_column_D33 = (sc_pc_column_D32 / sc_pc_aspen_model_results_E53) ** sc_pc_column_C9 * sc_pc_aspen_model_results_E54
        sc_pc_column_D34 = sc_pc_column_D33 * 1.38
        sc_pc_column_D35 = sc_pc_column_D34 * cost_indx_value_from_UserInput / sc_pc_cepci_D17 
        sc_pc_column_D15 = sc_pc_column_D35  
        sc_pc_capital_N17 = sc_pc_column_D15  
        sc_pc_result_summary_absorber = sc_pc_capital_N17 / 1000  

        # Display output in Streamlit
        st.write(int(round(sc_pc_result_summary_absorber, 0)))
        st.write("Absorber Result: 122,684 for placement. 19 for 400")



        # Calculating NGCC Stripper CO2 Removal BEC Breakdown                                                                              
        sc_pc_process_J24 = 0.99
        sc_pc_process_D18 = 0.1358
        sc_pc_stripper_sizing_D19 = 1069
        sc_pc_stripper_sizing_D23 = 12
        sc_pc_stripper_sizing_D31 = 3.6
        sc_pc_aspen_model_results_E59 = 3
        sc_pc_aspen_model_results_E57 = 40
        sc_pc_column_D41 = 1
        sc_pc_aspen_model_results_E62 = 17267660
        sc_pc_aspen_model_results_E58 = 48.5
        sc_pc_column_C9 = 0.9
        sc_pc_cepci_D17 = 581.8

        sc_pc_process_D24 = sc_pc_process_J24
        sc_pc_process_D35 = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
        sc_pc_stripper_sizing_D16 = sc_pc_process_D35 + flue_gas_fr_aft_dcc_UserInput * sc_pc_process_D18 * sc_pc_process_D24
        sc_pc_stripper_sizing_D17 = sc_pc_stripper_sizing_D16 / 3600
        sc_pc_stripper_sizing_D20 = (sc_pc_stripper_sizing_D17 * 1000 * 60) / (sc_pc_stripper_sizing_D19)
        sc_pc_stripper_sizing_D29 = 2 * sc_pc_stripper_sizing_D23 * sc_pc_stripper_sizing_D20 
        sc_pc_stripper_sizing_D18 = sc_pc_stripper_sizing_D16 * 2.20462 / 3600
        sc_pc_stripper_sizing_D30 = sc_pc_stripper_sizing_D29 * 0.001 * 35.3147
        sc_pc_stripper_sizing_D35 = sc_pc_stripper_sizing_D18 / sc_pc_stripper_sizing_D31
        sc_pc_stripper_sizing_D34 = sc_pc_stripper_sizing_D30 / sc_pc_stripper_sizing_D35
        sc_pc_stripper_sizing_D37 = math.sqrt(sc_pc_stripper_sizing_D35 / math.pi) * 2
        sc_pc_aspen_model_results_E60 = sc_pc_aspen_model_results_E57 + sc_pc_aspen_model_results_E59 * 8 + 15
        sc_pc_column_D39 = sc_pc_stripper_sizing_D34
        sc_pc_column_D40 = sc_pc_stripper_sizing_D37
