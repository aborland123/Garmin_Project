        def Calculate_NGCC_Other_Pumps():
            pump_and_blower_D84 = Flue_Gas_Flow_Rate_After_DCC_UserInput * L_G_Process_Parameters_UserInput
            NGCC_Pump_And_Blower_Lean_Pumps_Total_Flow_gpm = NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber / 60 * 2.2 / NGCC_Pump_And_Blower_Lean_Pumps_Lean_Solvent_Density_lbs_gallon
            pump_and_blower_D85 = pump_and_blower_D84 * NGCC_Worley_Solvent_Make_Up_Pumps_Flow_For_Pump_gpm / NGCC_Worley_Lean_Solvent_Flow_Rate_To_Absorber_kg_hr
            pump_and_blower_D96 = pump_and_blower_D95 * NGCC_Pump_And_Blower_Lean_Pumps_Total_Flow_gpm
            pump_and_blower_D86 = (pump_and_blower_D85 / NGCC_Worley_Solvent_Make_Up_Pumps_Flow_For_Pump_gpm) ** NGCC_Pump_And_Blower_Pump_Scale_Factor * NGCC_Worley_Solvent_Make_Up_Pumps_PEC_June_2011
            pump_and_blower_D97 = (pump_and_blower_D96 / NGCC_Worley_Water_Wash_Pump_Flow_For_Pump_gpm) ** NGCC_Pump_And_Blower_Pump_Scale_Factor * NGCC_Worley_Water_Wash_Pump_PEC_June_2011
            pump_and_blower_D88 = pump_and_blower_D86 * NGCC_Worley_Solvent_Make_Up_Pumps_BEC_For_Spare_Jun_2011 / NGCC_Worley_Solvent_Make_Up_Pumps_PEC_June_2011
            pump_and_blower_D87 = pump_and_blower_D86 * NGCC_Worley_Solvent_Make_Up_Pumps_BEC_Jun_2011 / NGCC_Worley_Solvent_Make_Up_Pumps_PEC_June_2011
            pump_and_blower_D99 = pump_and_blower_D97 * NGCC_Worley_Water_Wash_Pump_BEC_For_Spare_Jun_2011 / NGCC_Worley_Water_Wash_Pump_PEC_June_2011
            pump_and_blower_D98 = pump_and_blower_D97 * NGCC_Worley_Water_Wash_Pump_BEC_June_2011 / NGCC_Worley_Water_Wash_Pump_PEC_June_2011
            NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM = Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM_DOEB31B / NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B
            pump_and_blower_D84 = NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber
            pump_and_blower_D89 = pump_and_blower_D87 * pump_and_blower_D83 + pump_and_blower_D88
            pump_and_blower_D100 = pump_and_blower_D98 * pump_and_blower_D94 + pump_and_blower_D99
            pump_and_blower_D106 = NGCC_Worley_DCC_Pump_BEC_June_2011 * (NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM / NGCC_Worley_Flue_Gas_Flow_Rate_Before_DCC_ACFM) ** NGCC_Pump_And_Blower_Pump_Scale_Factor
            pump_and_blower_D112 = NGCC_Worley_DCC_Cost_Coal_Jun111 * (pump_and_blower_D84 / NGCC_Worley_Lean_Solvent_Flow_Rate_To_Absorber_kg_hr) ** NGCC_Pump_And_Blower_Pump_Scale_Factor
            pump_and_blower_D90 = pump_and_blower_D89 * Cost_Index_Value_From_UserInput / NGCC_Pump_And_Blower_2011_Jun
            pump_and_blower_D101 =pump_and_blower_D100 * Cost_Index_Value_From_UserInput / NGCC_Pump_And_Blower_2011_Jun
            pump_and_blower_D107 = pump_and_blower_D106 * Cost_Index_Value_From_UserInput / NGCC_Pump_And_Blower_2011_Jun
            pump_and_blower_D113 = pump_and_blower_D112 * Cost_Index_Value_From_UserInput / NGCC_Pump_And_Blower_2011_Jun
            return (pump_and_blower_D90 + pump_and_blower_D101 + pump_and_blower_D107 + pump_and_blower_D113) / (10 ** 3)


