            ngcc_result_summmary_C42 = res_lcoe_capital + ngcc_result_summmary_C44 + ngcc_result_summmary_C45 + res_lcoe_fuel
            ngcc_result_summmary_E39 = ngcc_process_D53 * 24 * 365 / 1000
            ngcc_result_summary_co2_captured_ex_t_s = (ngcc_result_summmary_E42 - ngcc_result_summmary_C42) / (ngcc_result_summmary_E39 / res_net_power_out_mw / ngcc_result_summmary_E41)
            ngcc_sensitivity_analysis_CO2_captured_baseline = ngcc_result_summary_co2_captured_ex_t_s
            return ngcc_sensitivity_analysis_CO2_captured_baseline


        # CO2 Captured Sensitivity Analysis 
        # ask if I need to add CPECI to this or if we just want to remove the input!!!
        co2_captured_sa_baseline = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_lr_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_lower_bound_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_lr_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_upper_bound_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_sup_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_lower_bound_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_sup_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_upper_bound_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_eng_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_eng_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_pc_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_pc_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_cpeci_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_cpeci_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_ng_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_lower_bound_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_ng_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_upper_bound_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_frc_low_boundary = co2_captured_analysis_baseline(fcr_lower_bound_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_frc_high_boundary = co2_captured_analysis_baseline(fcr_upper_bound_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_cf_low_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_lower_bound_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_cf_high_boundary = co2_captured_analysis_baseline(fcr_baseline_user_input, cf_upper_bound_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        # CO2 Avoided Sensitivity Analysis

        #baseline
        co2_avoided_sa_baseline = 10 + co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        # need to think of a way for Transport and Storage Cost
        co2_captured_sa_lr_low_boundary = co2_captured_sa_lr_low_boundary + 10
        co2_captured_sa_lr_high_boundary = co2_captured_sa_lr_high_boundary + 10

        co2_captured_sa_sup_low_boundary = co2_captured_sa_sup_low_boundary + 10
        co2_captured_sa_sup_high_boundary = co2_captured_sa_sup_high_boundary + 10

        co2_captured_sa_eng_low_boundary = co2_captured_sa_eng_low_boundary + 10
        co2_captured_sa_eng_high_boundary = co2_captured_sa_eng_high_boundary + 10

        co2_captured_sa_pc_low_boundary = co2_captured_sa_pc_low_boundary + 10
        co2_captured_sa_pc_high_boundary = co2_captured_sa_pc_high_boundary + 10

        #TSC
        co2_captured_sa_tsc_low_boundary = 5 + co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)
        co2_captured_sa_tsc_high_boundary = 20 + co2_captured_analysis_baseline(fcr_baseline_user_input, cf_baseline_user_input, ng_baseline_user_input, sup_baseline_user_input, lr_baseline_user_input, tsc_baseline_user_input, eng_baseline_user_input, pc_baseline_user_input)

        co2_captured_sa_cpeci_low_boundary = co2_captured_sa_cpeci_low_boundary + 10
        co2_captured_sa_cpeci_high_boundary = co2_captured_sa_cpeci_high_boundary + 10

        co2_captured_sa_ng_low_boundary = co2_captured_sa_ng_low_boundary + 10
        co2_captured_sa_ng_high_boundary = co2_captured_sa_ng_high_boundary + 10

        co2_captured_sa_frc_low_boundary = co2_captured_sa_frc_low_boundary + 10
        co2_captured_sa_frc_high_boundary = co2_captured_sa_frc_high_boundary + 10

        co2_captured_sa_cf_low_boundary = co2_captured_sa_cf_low_boundary + 10
        co2_captured_sa_cf_high_boundary = co2_captured_sa_cf_high_boundary + 10



        # If "Calculate Sensitivity" button is clicked than output this...
        if st.session_state["show_calculate_sensitivity_outputs_ngcc"]:
