        def Calculate_NGCC_Miscellaneous_CO2_Removal():
            proc_lean_solv_fr_absor = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput 
            aspen_lean_solv_flow_to_absorber = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
            hx_reboiler_duty_mmbtu_hr = hx_aspen_cross_duty_mmbtu_hr * (proc_lean_solv_fr_absor / aspen_lean_solv_flow_to_absorber)
            doe_baseline_inputs_F51 = doe_baseline_inputs_F50 * doe_baseline_inputs_F49 / (10 ** 6) + 4.86
            ngcc_inputs_C51 = hx_reboiler_duty_mmbtu_hr + 5
            ngcc_inputs_C49 = ngcc_inputs_C51 / doe_baseline_inputs_F51 * doe_baseline_inputs_F49
            other_equipment_D63 = worl_cond_tank_pec_each * (ngcc_inputs_C49 / worl_cond_tank_stream_ext_rate_coal) ** worl_cond_tank_sf
            lean_pump_tot_flow_gpm = proc_lean_solv_fr_absor / 60 * 2.2 / pump_lean_solv_dens_lbs_gal
            proc_lean_solv_fr_absor = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
            other_equipment_D64 = other_equipment_D63 * worl_cond_tank_sf_pec_bec
            other_equipment_D58 = worl_solv_rec_tank_pec_cost * (lean_pump_tot_flow_gpm / worl_solv_rec_tank_fr) ** worl_solv_rec_tank_sf
            other_equipment_D53 = worl_solv_makeup_tank_bec_jun_2011 * proc_lean_solv_fr_absor / worl_lean_solv_fr_absor_kg_hr
            other_equipment_D48 = worl_misc_bec_cost_jun_2011 * proc_lean_solv_fr_absor / worl_lean_solv_fr_absor_kg_hr
            cost_indx_value_from_UserInput = cost_indx_value_from_UserInput
            other_equipment_D66 = other_equipment_D64 * cost_indx_value_from_UserInput / cepci_cost_index_2018_q1_avg
            other_equipment_D59 = other_equipment_D58 * worl_solv_rec_tank_sf_pec_bec
            other_equipment_D54 = other_equipment_D53 * doe_base_b12b_2015_fuel_feed_rate / doe_base_b12b_2019_fuel_feed_rate * cepci_D16 / cepci_2011_jun
            other_equipment_D49 = other_equipment_D48 * doe_base_b12b_2015_fuel_feed_rate / doe_base_b12b_2019_fuel_feed_rate
            other_equipment_D67 = other_equipment_D65 * other_equipment_D66
            other_equipment_D60 = other_equipment_D59 * cost_indx_value_from_UserInput / cepci_cost_index_2018_q1_avg
            other_equipment_D55 = other_equipment_D54 * cost_indx_value_from_UserInput / cepci_D16
            other_equipment_D50 = other_equipment_D49 * cost_indx_value_from_UserInput / cepci_2011_jun
            ngcc_capital_N52 = other_equipment_D50 + other_equipment_D55 + other_equipment_D60 + other_equipment_D67
            return ngcc_capital_N52 / (10 ** 3)


