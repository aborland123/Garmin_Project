        ngcc_capital_U56 = ngcc_capital_U52 + ngcc_capital_U53 + ngcc_capital_U54 + ngcc_capital_U55
        ngcc_inputs_D114 = ngcc_inputs_D99 - ngcc_inputs_D113
        ngcc_process_D37 = fcr_baseline_user_input # user input
        ngcc_capital_U58 = ngcc_capital_U56 + ngcc_capital_U50 + ngcc_capital_U46 + ngcc_capital_V33
        ngcc_capital_V63 = ngcc_capital_U58 * ngcc_capital_V62
        ngcc_result_summmary_E38 = ngcc_process_D38
        ngcc_result_summmary_E41 = 8760
        ngcc_result_summmary_E11 = ngcc_inputs_D114
        ngcc_result_summmary_E37 = ngcc_process_D37
        ngcc_result_summmary_E36 = ngcc_capital_V63 / 1000
        ngcc_result_summmary_E43 = ngcc_result_summmary_E36 * 1000 * ngcc_result_summmary_E37 / (ngcc_result_summmary_E11 * ngcc_result_summmary_E41 * ngcc_result_summmary_E38)
        ngcc_result_summary_lcoe_capital = ngcc_result_summmary_E43

        st.write(f"LCOE Capital value is {ngcc_result_summary_lcoe_capital} should be 25.3 -113.1")



        #LCOE - Fixed op

        ngcc_result_summmary_E44 = ngcc_om_AD27 / (ngcc_result_summmary_E11 * ngcc_result_summmary_E41 * ngcc_result_summmary_E38)
        ngcc_result_summary_lcoe_fixed_op = ngcc_result_summmary_E44

        st.write(f"LCOE Fixed op value is {ngcc_result_summary_lcoe_fixed_op} should be 8.7 -1318.1")


