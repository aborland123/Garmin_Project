        sc_pc_column_D42 = sc_pc_column_D41 * (sc_pc_column_D40 / 2) ** 2 * math.pi * sc_pc_column_D39
        sc_pc_column_D43 = (sc_pc_column_D42 / sc_pc_aspen_model_results_E61) ** sc_pc_column_C9 * sc_pc_aspen_model_results_E62
        sc_pc_column_D44 = sc_pc_column_D43 * 1.45
        sc_pc_result_summary_stripper = (sc_pc_column_D44 * cost_indx_value_from_UserInput / sc_pc_cepci_D17) / 1000  

