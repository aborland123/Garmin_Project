                    title="LCOE ($/MWH)",
                    template="plotly_white",
                    showlegend=False
                )

                st.plotly_chart(fig, use_container_width=True)


                fig = go.Figure(go.Waterfall(
                    name="CO2 Captured Cost Breakdown",
                    orientation="v",
                    measure=["relative", "relative", "relative", "relative", "relative", "total"],
                    x=right_labels_co2,
                    y=right_values_co2,
                    connector={"line": {"color": "light gray"}},
                    increasing={"marker": {"color": "#54b5ce"}},
                    decreasing={"marker": {"color": "#f3a93a"}},
                    totals={"marker": {"color": "#40a816"}}
                ))

                fig.update_layout(
                    title="CO2 Captured Cost ($/tonne)",
                    template="plotly_white",
                    showlegend=False
                )

                st.plotly_chart(fig, use_container_width=True)


            
            except Exception as e:
                st.error(f"{e}")

    except Exception as e:
                st.error(f"{e}")

# Calculate Sensativity Analysis

#CO2 Captured Sensitivity Analysis Baseline #
def co2_captured_analysis_baseline(ngcc_process_D37, ngcc_process_D38, ngcc_process_D39, ngcc_process_D40, 
                                   ngcc_process_D41, ngcc_process_D42, ngcc_process_D43, ngcc_process_D44):
    ngcc_capital_V62 = 1.093
    flue_gas_fr_aft_dcc_UserInput = flue_gas_fr_aft_dcc_UserInput
    proc_co2_mass_frac_aftr_dcc = 0.0639
    co2_capture_rate_user_input = co2_capture_rate_user_input
    ngcc_process_D53 = flue_gas_fr_aft_dcc_UserInput * proc_co2_mass_frac_aftr_dcc * co2_capture_rate_user_input
    cost_indx_value_from_UserInput = cost_indx_value_from_UserInput
    proc_ref_flue_gas_aftr_dcc_kg_hr = 3889650
    ngcc_process_E17 = 0.0639
    ngcc_process_E22 = 0.90
    l_g_proc_param_UserInput = l_g_proc_param_UserInput
    ngcc_process_E53 = proc_ref_flue_gas_aftr_dcc_kg_hr * ngcc_process_E17 * ngcc_process_E22
    proc_lean_solv_fr_absor = flue_gas_fr_aft_dcc_UserInput * l_g_proc_param_UserInput
    proc_ref_flue_gas_before_dcc_doeb31b = 3927398
    proc_flue_gas_b4_dcc_acfm_DOEB31B = 2531705
    proc_flue_gas_b4_dcc_acfm = flue_gas_fr_aft_dcc_UserInput * proc_flue_gas_b4_dcc_acfm_DOEB31B / proc_ref_flue_gas_aftr_dcc_kg_hr
    proc_co2_mol_frac_aftr_dcc = 0.0414
    cepci_D16 = 615.9
    pump_lean_solv_dens_lbs_gal = 8.78120532592598
    lean_pump_tot_flow_gpm = proc_lean_solv_fr_absor / 60 * 2.2 / pump_lean_solv_dens_lbs_gal
    ngcc_om_N40 = lean_pump_tot_flow_gpm * 522 / 2 / 2000 * 0.30 * 1.5
    ngcc_capital_C4 = cost_indx_value_from_UserInput
    ngcc_capital_C5 = cost_indx_value_from_UserInput
    doe_NGCC_capital_I52 = 1212
    doe_NGCC_capital_I53 = 1252
    doe_NGCC_capital_I54 = 316
    doe_NGCC_capital_I55 = 2072
    doe_NGCC_capital_I56 = 105
    doe_NGCC_capital_I57 = 2001
    doe_NGCC_capital_I58 = 1612
    doe_NGCC_capital_I60 = 4661
    doe_NGCC_capital_L52 = 1091
    doe_NGCC_capital_L53 = 1502
    doe_NGCC_capital_L54 = 285
    doe_NGCC_capital_L55 = 2487
    doe_NGCC_capital_L56 = 94
    doe_NGCC_capital_L57 = 1801
    doe_NGCC_capital_L58 = 1935
    doe_NGCC_capital_D52 = 1696
    doe_NGCC_capital_D53 = 3756
    doe_NGCC_capital_D54 = 965
    doe_NGCC_capital_D55 = 1140
    doe_NGCC_capital_D56 = 230
    doe_NGCC_capital_D57 = 9304
    doe_NGCC_capital_D58 = 4998
    doe_NGCC_capital_D60 = 14217
    doe_NGCC_capital_E52 = 2908
    doe_NGCC_capital_E53 = 376
    doe_NGCC_capital_E54 = 316
    doe_NGCC_capital_E55 = 2176
    doe_NGCC_capital_E56 = 84
    doe_NGCC_capital_E57 = 400
    doe_NGCC_capital_E58 = 0
    doe_NGCC_capital_E60 = 1865
    doe_NGCC_capital_F52 = 1454
    doe_NGCC_capital_F53 = 2128
    doe_NGCC_capital_F54 = 301
    doe_NGCC_capital_F55 = 7046
    doe_NGCC_capital_F56 = 209
    doe_NGCC_capital_F57 = 300
    doe_NGCC_capital_F58 = 3063
    doe_NGCC_capital_F60 = 7225
    doe_NGCC_capital_G52 = 0
    doe_NGCC_capital_G53 = 0
    doe_NGCC_capital_G54 = 0
    doe_NGCC_capital_G55 = 0
    doe_NGCC_capital_G56 = 0
    doe_NGCC_capital_G57 = 0
    doe_NGCC_capital_G58 = 0
    doe_NGCC_capital_G60 = 0
    doe_NGCC_capital_H52 = doe_NGCC_capital_D52 + doe_NGCC_capital_E52 + doe_NGCC_capital_F52 + doe_NGCC_capital_G52
    doe_NGCC_capital_H53 = doe_NGCC_capital_D53 + doe_NGCC_capital_E53 + doe_NGCC_capital_F53 + doe_NGCC_capital_G53
    doe_NGCC_capital_H54 = doe_NGCC_capital_D54 + doe_NGCC_capital_E54 + doe_NGCC_capital_F54 + doe_NGCC_capital_G54
    doe_NGCC_capital_H55 = doe_NGCC_capital_D55 + doe_NGCC_capital_E55 + doe_NGCC_capital_F55 + doe_NGCC_capital_G55
    doe_NGCC_capital_H56 = doe_NGCC_capital_D56 + doe_NGCC_capital_E56 + doe_NGCC_capital_F56 + doe_NGCC_capital_G56
    doe_NGCC_capital_H57 = doe_NGCC_capital_D57 + doe_NGCC_capital_E57 + doe_NGCC_capital_F57 + doe_NGCC_capital_G57
    doe_NGCC_capital_H58 = doe_NGCC_capital_D58 + doe_NGCC_capital_E58 + doe_NGCC_capital_F58 + doe_NGCC_capital_G58
    doe_NGCC_capital_H60 = doe_NGCC_capital_D60 + doe_NGCC_capital_E60 + doe_NGCC_capital_F60 + doe_NGCC_capital_G60
    doe_NGCC_capital_L60 = 5594
    doe_NGCC_capital_O52 = doe_NGCC_capital_H52 + doe_NGCC_capital_I52 + doe_NGCC_capital_L52
    doe_NGCC_capital_O53 = doe_NGCC_capital_H53 + doe_NGCC_capital_I53 + doe_NGCC_capital_L53
    doe_NGCC_capital_O54 = doe_NGCC_capital_H54 + doe_NGCC_capital_I54 + doe_NGCC_capital_L54
    doe_NGCC_capital_O55 = doe_NGCC_capital_H55 + doe_NGCC_capital_I55 + doe_NGCC_capital_L55
    doe_NGCC_capital_O56 = doe_NGCC_capital_H56 + doe_NGCC_capital_I56 + doe_NGCC_capital_L56
    doe_NGCC_capital_O57 = doe_NGCC_capital_H57 + doe_NGCC_capital_I57 + doe_NGCC_capital_L57
    doe_NGCC_capital_O58 = doe_NGCC_capital_H58 + doe_NGCC_capital_I58 + doe_NGCC_capital_L58
    doe_NGCC_capital_O59 = 0
