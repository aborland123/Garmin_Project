        pump_lean_pumps_total_dp_psi = 1095 * 9.8 * (absorber_total_pack_height_ft - 20) * 0.3048 * 0.000145038 + worl_lean_pumps_dp_others_psi
        pump_lean_pumps_flow_per_pump_gpm = lean_pump_tot_flow_gpm / pump_lean_pump_num_psi
