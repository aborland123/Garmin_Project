        def Calculate_NGCC_Rich_Pump_CO2_Removal():
            NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber = Flue_Gas_Flow_Rate_After_DCC_UserInput * L_G_Process_Parameters_UserInput
            NGCC_Pump_And_Blower_Rich_Pumps_Total_Flow_gpm = (Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_CO2_Mass_Frac_After_Dcc * (1 - co2_capture_rate_user_input) + NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber) / 60 * 2.2 / NGCC_Pump_And_Blower_Rich_Solvent_Density_lbs_gallon
            NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_hr = NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber + Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_CO2_Mol_Frac_After_DCC * co2_capture_rate_user_input
            NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_s = NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_hr / 3600
            NGCC_Stripper_Liquid_Flow_Rate_Liquid_Flow_Rate_l_min = (NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_s * 1000 * 60) / (NGCC_Stripper_Liguid_Flow_Rate_Density_kg_m3)
            NGCC_Stripper_Volume_Of_Vessel_L = 2 * NGCC_Stripper_Operating_Conditions_Residence_Time_min * NGCC_Stripper_Liquid_Flow_Rate_Liquid_Flow_Rate_l_min
            NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_lbs_s = NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_kg_hr * 2.20462 / 3600
            NGCC_Stripper_Dimension_Cross_Section_ft2 = NGCC_Stripper_Liquid_Flow_Rate_Rich_Solvent_Flow_To_Stripper_lbs_s / NGCC_Stripper_Volume_Of_Vessel_G_ft3
            NGCC_Stripper_Volume_Of_Vessel_ft3 = NGCC_Stripper_Volume_Of_Vessel_L * 0.001 * 35.3147
            NGCC_Stripper_Total_Height_ft = NGCC_Stripper_Volume_Of_Vessel_ft3 / NGCC_Stripper_Dimension_Cross_Section_ft2
            NGCC_Worley_Rich_Pumps_Dp_Of_Column_psi = 1095 * NGCC_Worley_Other_Equipment_Cost_Constant_g * (NGCC_Worley_Rich_Pumps_Stripper_ft) * NGCC_Worley_Other_Equipment_Cost_Constant_Convert_ft_to_m * NGCC_Worley_Other_Equipment_Cost_Constant_Convert_pa_to_psi
            NGCC_Worley_Rich_Pumps_Dp_From_Others_psi = NGCC_Worley_Rich_Pumps_Total_Dp_psi - NGCC_Worley_Rich_Pumps_Dp_Of_Column_psi
            NGCC_Pump_And_Blower_Rich_Pumps_Other_Pressure_Drop_psi = NGCC_Worley_Rich_Pumps_Dp_From_Others_psi
            NGCC_Pump_And_Blower_Rich_Pumps_Flow_For_Each_gpm = NGCC_Pump_And_Blower_Rich_Pumps_Total_Flow_gpm / NGCC_Pump_And_Blower_Rich_Pumps_Number_Of_Rich_Pump_Operation
            NGCC_Pump_And_Blower_Rich_Pumps_Total_Dp_psi = 1095 * 9.8 * (NGCC_Stripper_Total_Height_ft) * 0.3048 * 0.000145038 + NGCC_Pump_And_Blower_Rich_Pumps_Other_Pressure_Drop_psi
            NGCC_Pump_And_Blower_Rich_Pumps_Work_For_Each_hp = NGCC_Pump_And_Blower_Rich_Pumps_Flow_For_Each_gpm * NGCC_Pump_And_Blower_Rich_Pumps_Total_Dp_psi / 1714 / NGCC_Pump_And_Blower_Rich_Pumps_Efficiency_Percent
            NGCC_Worley_Rich_Pumps_Work_hp = NGCC_Worley_Rich_Pumps_Dp_psi * NGCC_Worley_Rich_Pumps_Flow_gpm / 1714
            NGCC_Pump_And_Blower_Rich_Pumps_PEC_For_Each_June_2011 = (NGCC_Pump_And_Blower_Rich_Pumps_Work_For_Each_hp / NGCC_Worley_Rich_Pumps_Work_hp) ** NGCC_Pump_And_Blower_Pump_Scale_Factor * NGCC_Worley_Rich_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Rich_Pumps_BEC_For_Each_Spare_June_2011 = NGCC_Pump_And_Blower_Rich_Pumps_PEC_For_Each_June_2011 * NGCC_Worley_Rich_Pumps_BEC_For_Spare_Jun_2011 / NGCC_Worley_Rich_Pumps_PEC_June_2011 
            NGCC_Pump_And_Blower_Rich_Pumps_BEC_For_Each_Op_June_2011 = NGCC_Pump_And_Blower_Rich_Pumps_PEC_For_Each_June_2011 * NGCC_Worley_Rich_Pumps_BEC_June_2011 / NGCC_Worley_Rich_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Rich_Pumps_Total_BEC_June_2011 = NGCC_Pump_And_Blower_Rich_Pumps_BEC_For_Each_Op_June_2011 * NGCC_Pump_And_Blower_Rich_Pumps_Number_Of_Rich_Pump_Operation + NGCC_Pump_And_Blower_Rich_Pumps_BEC_For_Each_Spare_June_2011
            NGCC_Pump_And_Blower_Rich_Pumps_Total_BEC_Dec_2018 = NGCC_Pump_And_Blower_Rich_Pumps_Total_BEC_June_2011 * Cost_Index_Value_From_UserInput / NGCC_Pump_And_Blower_2011_Jun
            return NGCC_Pump_And_Blower_Rich_Pumps_Total_BEC_Dec_2018 / 1000

