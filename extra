            NGCC_HX_Cross_HX_HX_Area_ft2 = NGCC_HX_Cross_HX_Duty_MMBtu_hr / NGCC_Worley_HX_LMTD_F / NGCC_Worley_Reboiler_OHTC_Btu_ft2hF * 1000000
            NGCC_HX_Cross_HX_Number_Of_HX = math.ceil(NGCC_HX_Cross_HX_HX_Area_ft2 / 17000) 
