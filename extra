    strip_vessel_vol_ft3 = strip_vol_vessel_l * 0.001 * 35.3147
    stripper_dimension_length_ft = strip_vessel_vol_ft3 / strip_cross_sec_ft2
    strip_total_height_ft = stripper_dimension_length_ft
