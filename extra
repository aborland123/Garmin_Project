            NGCC_HX_DCC_Cooler_Total_BEC_Dec_2018 = NGCC_HX_DCC_Cooler_Total_BEC_June_2011 * Cost_Index_Value_From_UserInput / NGCC_HX_Assumption_2011_Jun
            NGCC_HX_Lean_Cooler_Total_BEC_Dec_2018 = NGCC_HX_Lean_Cooler_Total_BEC_June_2011 * Cost_Index_Value_From_UserInput / NGCC_HX_Assumption_2011_Jun
            NGCC_HX_Intercooler_Total_BEC_Dec_2018 = NGCC_HX_Intercooler_Total_BEC_June_2011 * Cost_Index_Value_From_UserInput / NGCC_HX_Assumption_2011_Jun
            return (NGCC_HX_Intercooler_Total_BEC_Dec_2018 + NGCC_HX_Lean_Cooler_Total_BEC_Dec_2018 + NGCC_HX_DCC_Cooler_Total_BEC_Dec_2018 + NGCC_HX_WW_Cooler_Total_BEC_Dec_2018) / 1000 


        # Display output in Streamlit
        st.write(Calculate_NGCC_All_Other_HXs_CO2_Removal())
        st.write("All other HX's Result: 3,644 for placement. 2,855 for 400)")


        # Calculating NGCC Lean Pump CO2 Removal BEC Breakdown
        NGCC_Pump_And_Blower_Lean_Pumps_Efficiency_Percent = 0.83
        NGCC_Pump_And_Blower_Lean_Pumps_Number_Of_Lean_Pump_psi = 2
        NGCC_Pump_And_Blower_Lean_Pumps_Lean_Solvent_Density_lbs_gallon = 8.78120532592598
        NGCC_Worley_Lean_Pumps_Total_Dp_Psi = 68.5
        NGCC_Worley_Other_Equipment_Cost_Constant_g = 9.8
        NGCC_Worley_Lean_Pumps_Absorber_ft = 88
        NGCC_Worley_Other_Equipment_Cost_Constant_Convert_ft_to_m = 0.3048
        NGCC_Worley_Other_Equipment_Cost_Constant_Convert_pa_to_psi = 0.000145038
        NGCC_Absorber_WW_Packing_Height_Ft = 10
        NGCC_Worley_Lean_Pumps_dp_psi = 46.4
        NGCC_Worley_Lean_Pumps_flow_gpm = 22676
        NGCC_Pump_And_Blower_Pump_Scale_Factor = 0.72
        NGCC_Worley_Lean_Pumps_BEC_For_Spare_Jun_2011 = 2191808.24
        NGCC_Worley_Lean_Pumps_BEC_Jun_2011 = 3181081.04
        NGCC_Worley_Lean_Pumps_PEC_June_2011 = 1038544
        NGCC_Pump_And_Blower_2011_Jun = 588.9

        def Calculate_NGCC_Lean_Pump_CO2_Removal():
            NGCC_Worley_Lean_Pumps_Dp_Of_Column_psi = 1095 * NGCC_Worley_Other_Equipment_Cost_Constant_g * (NGCC_Worley_Lean_Pumps_Absorber_ft) * NGCC_Worley_Other_Equipment_Cost_Constant_Convert_ft_to_m * NGCC_Worley_Other_Equipment_Cost_Constant_Convert_pa_to_psi
            NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber = Flue_Gas_Flow_Rate_After_DCC_UserInput * L_G_Process_Parameters_UserInput
            NGCC_Absorber_Fixed_Design_Packing_Height_ft = 130.95 * L_G_Process_Parameters_UserInput - 82.133
            NGCC_Absorber_Number_Packing_Section = round(NGCC_Absorber_Fixed_Design_Packing_Height_ft / 12, 0)
            NGCC_Absorber_Height_Packing_Height_ft = NGCC_Absorber_Fixed_Design_Packing_Height_ft + NGCC_Absorber_WW_Packing_Height_Ft + NGCC_Absorber_Number_Packing_Section * 10 + 25
            NGCC_Worley_Lean_Pumps_Dp_From_Others_psi = NGCC_Worley_Lean_Pumps_Total_Dp_Psi - NGCC_Worley_Lean_Pumps_Dp_Of_Column_psi
            NGCC_Pump_And_Blower_Lean_Pumps_Total_Flow_gpm = NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber / 60 * 2.2 / NGCC_Pump_And_Blower_Lean_Pumps_Lean_Solvent_Density_lbs_gallon
            NGCC_Pump_And_Blower_Lean_Pumps_Total_Dp_psi = 1095 * 9.8 * (NGCC_Absorber_Height_Packing_Height_ft - 20) * 0.3048 * 0.000145038 + NGCC_Worley_Lean_Pumps_Dp_From_Others_psi
            NGCC_Pump_And_Blower_Lean_Pumps_Flow_For_Each_gpm = NGCC_Pump_And_Blower_Lean_Pumps_Total_Flow_gpm / NGCC_Pump_And_Blower_Lean_Pumps_Number_Of_Lean_Pump_psi
            NGCC_Pump_And_Blower_Lean_Pumps_Work_For_Each_hp = NGCC_Pump_And_Blower_Lean_Pumps_Flow_For_Each_gpm * NGCC_Pump_And_Blower_Lean_Pumps_Total_Dp_psi / 1714 / NGCC_Pump_And_Blower_Lean_Pumps_Efficiency_Percent
            NGCC_Worley_Lean_Pumps_work_hp = NGCC_Worley_Lean_Pumps_dp_psi * NGCC_Worley_Lean_Pumps_flow_gpm / 1714
            NGCC_Pump_And_Blower_Lean_Pumps_PEC_For_Each_June_2011 = (NGCC_Pump_And_Blower_Lean_Pumps_Work_For_Each_hp / NGCC_Worley_Lean_Pumps_work_hp) ** NGCC_Pump_And_Blower_Pump_Scale_Factor * NGCC_Worley_Lean_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Lean_Pumps_BEC_For_Each_Op_June_2011 = NGCC_Pump_And_Blower_Lean_Pumps_PEC_For_Each_June_2011 * NGCC_Worley_Lean_Pumps_BEC_Jun_2011 / NGCC_Worley_Lean_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Lean_Pumps_BEC_For_Each_Spare_June_2011 = NGCC_Pump_And_Blower_Lean_Pumps_PEC_For_Each_June_2011 * NGCC_Worley_Lean_Pumps_BEC_For_Spare_Jun_2011 / NGCC_Worley_Lean_Pumps_PEC_June_2011
            NGCC_Pump_And_Blower_Lean_Pumps_Total_BEC_June_2011 = NGCC_Pump_And_Blower_Lean_Pumps_BEC_For_Each_Op_June_2011 * NGCC_Pump_And_Blower_Lean_Pumps_Number_Of_Lean_Pump_psi + NGCC_Pump_And_Blower_Lean_Pumps_BEC_For_Each_Spare_June_2011
            NGCC_Pump_And_Blower_Lean_Pumps_Total_BEC_Dec_2018 = NGCC_Pump_And_Blower_Lean_Pumps_Total_BEC_June_2011 * Cost_Index_Value_From_UserInput / NGCC_Pump_And_Blower_2011_Jun
            return NGCC_Pump_And_Blower_Lean_Pumps_Total_BEC_Dec_2018 / 1000


        st.write(Calculate_NGCC_Lean_Pump_CO2_Removal())
        st.write("Lean Pump Result: 8,513 for placement. 11 for 400)")




        # Calculating NGCC Rich Pump CO2 Removal BEC Breakdown                                                                  
