                st.plotly_chart(fig, use_container_width=True)


            
            except Exception as e:
                st.error(f"{e}")

    except Exception as e:
                st.error(f"{e}")

# Calculate Sensativity Analysis

#CO2 Captured Sensitivity Analysis Baseline #
def co2_captured_analysis_baseline(ngcc_process_D37, ngcc_process_D38, ngcc_process_D39, ngcc_process_D40, 
                                   ngcc_process_D41, ngcc_process_D42, ngcc_process_D43, ngcc_process_D44):
    ngcc_capital_V62 = 1.093
    Flue_Gas_Flow_Rate_After_DCC_UserInput = Flue_Gas_Flow_Rate_After_DCC_UserInput
    NGCC_Process_CO2_Mass_Frac_After_Dcc = 0.0639
    co2_capture_rate_user_input = co2_capture_rate_user_input
    ngcc_process_D53 = Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_CO2_Mass_Frac_After_Dcc * co2_capture_rate_user_input
    Cost_Index_Value_From_UserInput = Cost_Index_Value_From_UserInput
    NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B = 3889650
    ngcc_process_E17 = 0.0639
    ngcc_process_E22 = 0.90
    L_G_Process_Parameters_UserInput = L_G_Process_Parameters_UserInput
    ngcc_process_E53 = NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B * ngcc_process_E17 * ngcc_process_E22
    NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber = Flue_Gas_Flow_Rate_After_DCC_UserInput * L_G_Process_Parameters_UserInput
    NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_DOEB31B = 3927398
    NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM_DOEB31B = 2531705
    NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM = Flue_Gas_Flow_Rate_After_DCC_UserInput * NGCC_Process_Flue_Gas_Flow_Rate_Before_DCC_ACFM_DOEB31B / NGCC_Process_Flue_Gas_Flow_Rate_After_DCC_kg_hr_DOEB31B
    NGCC_Process_CO2_Mol_Frac_After_DCC = 0.0414
    cepci_D16 = 615.9
    NGCC_Pump_And_Blower_Lean_Pumps_Lean_Solvent_Density_lbs_gallon = 8.78120532592598
    NGCC_Pump_And_Blower_Lean_Pumps_Total_Flow_gpm = NGCC_Process_Lean_Solvent_Flow_Rate_To_Absorber / 60 * 2.2 / NGCC_Pump_And_Blower_Lean_Pumps_Lean_Solvent_Density_lbs_gallon
    ngcc_om_N40 = NGCC_Pump_And_Blower_Lean_Pumps_Total_Flow_gpm * 522 / 2 / 2000 * 0.30 * 1.5
    ngcc_capital_C4 = Cost_Index_Value_From_UserInput
    ngcc_capital_C5 = Cost_Index_Value_From_UserInput
    doe_NGCC_capital_I52 = 1212
    doe_NGCC_capital_I53 = 1252
    doe_NGCC_capital_I54 = 316
